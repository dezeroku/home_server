{{- with secret "kvv2/victoria-metrics/auth-config" -}}
users:
  # homekit
  - bearer_token: "{{ .Data.data.homekit_token }}"
    url_prefix: "http://victoria-metrics-victoria-metrics-single-server.victoria-metrics.svc.cluster.local:8428?extra_label=category=homekit"

  # test
  - username: "{{ .Data.data.test_username }}"
    password: "{{ .Data.data.test_password }}"
    url_prefix: "http://victoria-metrics-victoria-metrics-single-server.victoria-metrics.svc.cluster.local:8428"
{{- end -}}
